{
	"include": [
		"$(MODDABLE)/modules/drivers/ili9341/manifest.json",
		"$(MODULES)/drivers/led/manifest.json"
	],
	"modules": {
		"setup/target": "./setup-target"
	},
	"preload": "setup/target",
	"config": {
		"screen": "ili9341",
		"touch": ""
	},
	"defines": {
		"i2c": {
			"sda_pin": 4,
			"scl_pin": 5
		},
		"spi": {
			"miso_pin": 8,
			"mosi_pin": 11,
			"sck_pin": 10
		},
		"ili9341": {
			"hz": 40000000,
			"cs_pin": 9,
			"dc_pin": 7,
			"rst_pin": 12,
			"spi_port": 1,
			"backlight_on": 1
		}
	}
}
