{
	"modules": {
		"*": [
			"$(MODULES)/base/time/*",
			"$(MODULES)/base/time/esp/*",
			"$(MODULES)/base/timer/*",
			"$(MODULES)/base/timer/mc/*",
			"$(MODULES)/files/preference/modPreference",
			"$(MODULES)/files/preference/esp/modPreference"
		]
	},
	"defines": {
		"i2c": {
			"sda_pin": 4,
			"scl_pin": 5
		}
	},
	"platforms": {
		"pico/*": {
			"include": "$(BUILD)/devices/pico/targets/$(SUBPLATFORM)/manifest.json"
		}
	}
}
