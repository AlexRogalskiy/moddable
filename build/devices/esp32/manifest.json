{
	"modules": {
		"*": [
			"$(MODULES)/base/time/*",
			"$(MODULES)/base/time/esp/*",
			"$(MODULES)/base/timer/*",
			"$(MODULES)/base/timer/mc/*",
			"$(MODULES)/files/preference/modPreference",
			"$(MODULES)/files/preference/esp32/modPreference"
		]
	},
	"preload": [
		"preference",
		"time",
		"timer"
	],
	"platforms": {
		"esp32/*": {
			"include": "./targets/$(SUBPLATFORM)/manifest.json"
		}
	},
	"defines": {
		"spi": {
			"miso_pin": 12,
			"mosi_pin": 13,
			"sck_pin": 14
		}
	}
}
